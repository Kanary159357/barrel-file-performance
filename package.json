    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/testing-library__dom": "^7.5.0",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
