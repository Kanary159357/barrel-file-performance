  "scripts": {
    "build:webpack": "webpack --mode production",
    "build:webpack:barrel": "yarn build:webpack --entry ./src/barrel.tsx",
    "build:webpack:non-barrel": "yarn build:webpack --entry ./src/non-barrel.tsx",
    "build:webpack:swc": " webpack --mode production --config webpack.swc.config.js",
    "build:webpack:swc:barrel": "yarn build:webpack:swc --entry ./src/barrel.tsx",
    "build:webpack:swc:non-barrel": "yarn build:webpack:swc --entry ./src/non-barrel.tsx",
    "typecheck:barrel": "tsc --p tsconfig.barrel.json",
    "typecheck:non-barrel": "tsc --p tsconfig.non-barrel.json",
    "eslint:barrel": "eslint --config .eslintrc.barrel.js --ext .ts,.tsx ./src/barrel.tsx",
    "eslint:non-barrel": "eslint --config .eslintrc.non-barrel.js --ext .ts,.tsx ./src/non-barrel.tsx",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,css,md,json}'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
